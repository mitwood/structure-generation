[BASIS]
elements = ['W']
ranks = [1,2,3]
lmax = [0,3,3]
lmin = [0,0,0]
nmax = [8,1,1]
bikflag = 0
dgradflag = 0

[MOTION]
soft_strength = 0.0  #Repulsive potential
ml_strength = 1.0    #Loss function potential
nsteps = 1000
temperature = 0.0
min_type = "line" #line/temp/box/fire
randomize_comps = False # flag to use randomized compositions for elements in the dictionary: target_comps = {'Cr':1.0 }

[GENETIC]
mutation_rate = 0.50
mutation_types = {'perturb_one': 0.5, 'perturb_N' : 0.5, 'flip_one': 0.0, 'flip_N': 0.0, 'create_one' : 0.0, 'delete_one' : 0.0}
population_size = 20
ngenerations = 10

[SCORING]
q1_weight = 1.0    #Coeff on loss function mean term (should always be positive)
q2_weight = 0.0    #Coeff on loss function varience term (should always be positive)
cross_weight = 0.0   #closeness to target as a joint distribution min/max just flip this sign (pos being minimize).
self_weight = 0.50 #closeness to target within an atomic configuration. min/max just flip this sign (pos being minimize). 
q1_bonus = 0.0          # extra objective function variables to target exact matches of descriptors  
q1_cross_bonus = 0.0    # extra objective function variables to target exact matches of descriptors 
q2_bonus = 0.0          # extra objective function variables to target exact matches of descriptors 
q2_cross_bonus = 0.0    # extra objective function variables to target exact matches of descriptors 

[TARGET]
target_fname = '../bcc.data'
start_fname = '../bcc.data'

[CONSTRAINT]
type_map = {1:'W'}
masses = {1: 183.84}
target_comps = {'W':1.0} #composition in dictionary form: e.g. { W:0.5, Be:0.5} 
minatoms = 54
maxatoms = 55
template = None
use_template = None

[OUTPUT]
logfiles = fcctobcc_
structures = fcctobcc_
scores = fcctobcc_